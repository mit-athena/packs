#device			device		mount	FS	fsck	mount	mount
#to mount		to fsck		point	type	pass	at boot	options
#
/proc			-		/proc	proc	-	no	-
fd			-		/dev/fd	fd	-	no	-

/dev/dsk/@DISK@s0 /dev/rdsk/@DISK@s0	/	ufs	1	no	logging
/dev/dsk/@DISK@s3 /dev/rdsk/@DISK@s3 /usr/vice/cache ufs 5	yes	nologging
/dev/dsk/@DISK@s1	-		-	swap	-	no	-
