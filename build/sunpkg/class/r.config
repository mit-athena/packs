#!/bin/sh

# Handle the removal of config files in this package.  pkgrm supplies
# the path names on our standard input.  For each config file foo, we
# remove foo and foo.athena, but not foo.old.
while read dest ; do
  rm -f $dest $dest.athena
  if [ -f $dest.old ]; then
    echo "not removing $dest.old"
  fi
done

exit 0
