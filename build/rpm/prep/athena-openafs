#!/bin/sh

if [ linux = "$OS" ]; then
  cp $1/packs/config/os/$OS/CellAlias $2
  cp $1/packs/config/os/$OS/CellServDB $2
  cp $1/packs/config/os/$OS/ThisCell $2
  cp $1/packs/config/os/$OS/cacheinfo $2
  cp $1/packs/maint/os/$OS/system-afs.sh $2
fi
cp $1/packs/maint/config_afs.sh $2
cp $1/packs/maint/os/linux/system-afs.sh $2
