#!/bin/sh

echo "/etc/athena/attach.conf"				>> "$CONFCHG"

case "$HOSTTYPE" in
sun4)
	echo "MINIROOT=true"				>> "$CONFVARS"
	echo "NEWOS=true"				>> "$CONFVARS"
	echo "NEWDEVS=true"				>> "$CONFVARS"
	echo "OSCHANGES=true"				>> "$CONFVARS"

	echo "/etc/athena/named-bootconf"		>> "$DEADFILES"
	echo "/etc/athena/syslogd"			>> "$DEADFILES"
	echo "/etc/athena/xdm"				>> "$DEADFILES"
	echo "/usr/lib/cpp"				>> "$DEADFILES"
	echo "/usr/ccs/lib/cpp"				>> "$DEADFILES"
	cat "$LIBDIR/solaris26-7"			>> "$DEADFILES"

	# This file will be inaccurate after we remove the OS links.
	echo "/var/sadm/install/contents"		>> "$DEADFILES"

	# Create list of old packages and old patches to be removed
	# from the DB.
	cat "$LIBDIR/solaris26-pkgs"			>> "$OLDPKGS"
	cat "$LIBDIR/solaris26-ptchs"			>> "$OLDPTCHS"

	# Create the lists of packages and patches to install.
	cat "$LIBDIR/solaris7-local-$SUNPLATFORM"	>> "$LOCALPACKAGES"
	cat "$LIBDIR/solaris7-nolocal-$SUNPLATFORM"	>> "$LINKPACKAGES"
	cat "$LIBDIR/solaris7-patches.8.4.0"		>> "$PATCHES"
	;;

sgi)
	echo "/etc/athena/axdm"				>> "$DEADFILES"
	echo "/etc/athena/login/elmer"			>> "$DEADFILES"
	echo "/etc/athena/login/xdm"			>> "$DEADFILES"
	echo "/etc/athena/named-bootconf"		>> "$DEADFILES"
	echo "/etc/athena/nanny"			>> "$DEADFILES"
	echo "/etc/athena/syslogd"			>> "$DEADFILES"
	echo "/etc/init.d/axdm"				>> "$DEADFILES"

	rm -f /etc/init.d/axdm /etc/rc2.d/S98xdm /etc/rc0.d/K02xdm
	ln -s ../init.d/xdm /etc/rc2.d/S98xdm
	ln -s ../init.d/xdm /etc/rc0.d/K02xdm

	osrel=`uname -r`
	# Boot up the miniroot to upgrade the OS.
	echo "NEWOS=true"				>> "$CONFVARS"
	echo "MINIROOT=true"				>> "$CONFVARS"
	echo "OSCHANGES=true"				>> "$CONFVARS"

	# Hack to prevent inst from doing multiple make's
	# of the filetype database.
	echo "DELAYFTMAKE=true"				>> "$CONFVARS"

	# Some config files have changed.
	echo "/etc/inetd.conf"				>> "$CONFCHG"
	echo "/etc/services"				>> "$CONFCHG"
	echo "/etc/syslog.conf"				>> "$CONFCHG"
	echo "/var/spool/cron/crontabs/root"		>> "$CONFCHG"

	# If we're running a pre-6.5 OS, we will replace all existing
	# packages, so remove any old selections files.
	if [ "6.5" != "$osrel" ]; then
	    for dist in `cat /install/selections/dist_order 2>/dev/null` ; do
		rm -f "$LOCALPACKAGES.$dist" "$LINKPACKAGES.$dist"
	    done
	fi

	# Make a handy variable for local foundation package selections.
	foundlocal="$LOCALPACKAGES.foundation"

	# Allow us to remove any installed package.
	echo "set removepolicy anything"		>> "$foundlocal"

	if [ "6.5" = "$osrel" ]; then
	    # Override intermediate conflicts.
	    echo "set rulesoverride true"		>> "$foundlocal"

	    # These conflict with the new cosmo software.
	    echo "remove I cosmoplayer.sw.chinese_china" \
							>> "$foundlocal"
	    echo "remove I cosmoplayer.sw.chinese_taiwan" \
							>> "$foundlocal"
	    echo "remove I cosmoplayer.sw.japanese"	>> "$foundlocal"
	    echo "remove I cosmoworlds.sw.chinese_china" \
							>> "$foundlocal"
	    echo "remove I cosmoworlds.sw.chinese_taiwan" \
							>> "$foundlocal"
	    echo "remove I cosmoworlds.sw.japanese"	>> "$foundlocal"

	    # These packages go away in the update.
	    echo "remove I SpeedShop.sw.WorkShop"	>> "$foundlocal"
	    echo "remove I cosmoplayer.man.relnotes"	>> "$foundlocal"
	    echo "remove I netscape.plugin.PNG"		>> "$foundlocal"
	    echo "remove I netscape.plugin.XPM"		>> "$foundlocal"
	    echo "remove I netscape.plugin32.PNG"	>> "$foundlocal"
	    echo "remove I netscape.plugin32.XPM"	>> "$foundlocal"
	    echo "remove I patchSG0003692.eoe_sw.base"	>> "$foundlocal"
	    echo "remove I patchSG0003800.nfs_sw.nfs"	>> "$foundlocal"

	    # SGIMeeting is now a licensed product.
	    echo "remove I sgimeeting"			>> "$foundlocal"

	    # The following should get updated, but have packages that get
	    # deselected somehow in the dev symlink pass, thus causing
	    # conflict.  Nuking them here, and installing them later as
	    # new packages, avoids the problem.
	    echo "remove I SpeedShop"			>> "$foundlocal"
	    echo "remove I apo_doc"			>> "$foundlocal"
	    echo "remove I c++_dev"			>> "$foundlocal"
	    echo "remove I c++_fe"			>> "$foundlocal"
	    echo "remove I compiler_dev"		>> "$foundlocal"
	    echo "remove I ftn_dev"			>> "$foundlocal"

	else
	    # The current OS is pre-6.5.  Remove all currently installed
	    # packages.
	    echo "remove I"				>> "$foundlocal"

	fi

	# Install all desired "foundation" local packages.
	echo "i c++_eoe.sw.lib"				>> "$foundlocal"
	echo "i compiler_eoe.sw.cpp"			>> "$foundlocal"
	echo "i compiler_eoe.sw.lboot"			>> "$foundlocal"
	echo "i compiler_eoe.sw.lib"			>> "$foundlocal"
	echo "i compiler_eoe.sw.unix"			>> "$foundlocal"
	echo "i dmedia_eoe.sw.base"			>> "$foundlocal"
	echo "i eoe.sw.base"				>> "$foundlocal"
	echo "i eoe.sw.efs"				>> "$foundlocal"
	echo "i eoe.sw.gfx"				>> "$foundlocal"
	echo "i x_eoe.sw.Server"			>> "$foundlocal"
	echo "i x_eoe.sw.Xfonts"			>> "$foundlocal"
	echo "i x_eoe.sw.eoe"				>> "$foundlocal"
	echo "i dmedia_eoe.sw.audio"			>> "$foundlocal"
	echo "i dmedia_eoe.sw.lib"			>> "$foundlocal"
	echo "i eoe.sw.named"				>> "$foundlocal"
	echo "i nfs.sw.nfs"				>> "$foundlocal"
	echo "i eoe.sw.terminfo"			>> "$foundlocal"
	echo "i eoe.sw.termbase"			>> "$foundlocal"
	echo "i x_eoe.sw.Server"			>> "$foundlocal"
	echo "i x_eoe.sw.Xapps"				>> "$foundlocal"
	echo "i x_eoe.sw.Xfonts"			>> "$foundlocal"
	echo "i x_eoe.sw.Xgifts"			>> "$foundlocal"
	echo "i x_eoe.sw.Xoptfonts"			>> "$foundlocal"
	echo "i x_eoe.sw.Xopttype1"			>> "$foundlocal"
	echo "i x_eoe.sw.Xunicodefonts"			>> "$foundlocal"
	echo "i x_eoe.sw.eoe"				>> "$foundlocal"
	echo "i x_eoe.sw.pex"				>> "$foundlocal"
	echo "i x_eoe.sw.plugin"			>> "$foundlocal"
	echo "i x_eoe.sw.xfwp"				>> "$foundlocal"
	echo "i desktop_base.sw.FileTypingRules"	>> "$foundlocal"
	echo "i desktop_base.sw.dso"			>> "$foundlocal"
	echo "i desktop_base.sw.ftrdso6_2"		>> "$foundlocal"
	echo "i desktop_base.sw.utilities"		>> "$foundlocal"
	echo "i ViewKit_eoe.sw.base"			>> "$foundlocal"
	echo "i 4Dwm.sw.4Dwm"				>> "$foundlocal"
	echo "i motif_eoe.sw.eoe"			>> "$foundlocal"
	echo "i websupport_eoe.sw.lib"			>> "$foundlocal"
	echo "i ifl_eoe.sw.c++"				>> "$foundlocal"
	echo "i il_eoe.sw.c++"				>> "$foundlocal"
	echo "i insight_base.sw.eoe"			>> "$foundlocal"
	echo "i insight_base.sw.base"			>> "$foundlocal"
	echo "i isdn_eoe.sw.isdn"			>> "$foundlocal"
	echo "i eoe.sw.ppp"				>> "$foundlocal"
	echo "i eoe.sw.uucp"				>> "$foundlocal"
	# This is required on O2, but not installable on Indy.
	echo "i A ip32prom.sw.prom"			>> "$foundlocal"

	if [ "6.5" = "$osrel" ]; then
	    # Deselect packages that are the same version or a downgrade
	    # from what is installed.
	    echo "k same"				>> "$foundlocal"
	    echo "k D"					>> "$foundlocal"
	fi

	# Install all desired "applications" local packages.
	applocal="$LOCALPACKAGES.applications"
	echo "i x_eoe.sw.xdps"				>> "$applocal"
	echo "i dps_eoe.sw.dpsfonts"			>> "$applocal"
	echo "i desktop_eoe.sw.Confidence"		>> "$applocal"
	echo "i desktop_eoe.sw.Desks"			>> "$applocal"
	echo "i desktop_eoe.sw.control_panels"		>> "$applocal"
	echo "i desktop_eoe.sw.envm"			>> "$applocal"
	echo "i desktop_eoe.sw.share"			>> "$applocal"
	echo "i desktop_eoe.sw.toolchest"		>> "$applocal"
	echo "i desktop_base.sw.dso6_2"			>> "$applocal"
	echo "i media_warehouse.sw.viewers"		>> "$applocal"
	echo "i webviewer.sw.eoe"			>> "$applocal"
	# Deselect packages that are the same version or a downgrade
	# from what is installed.
	echo "k same"					>> "$applocal"
	echo "k D"					>> "$applocal"

	# Install local patches.
	patchlocal="$LOCALPACKAGES.patches"
	echo "i patchSG0003913.eoe_sw.base"		>> "$patchlocal"
	# Deselect packages that are the same version or a downgrade
	# from what is installed.
	echo "k same"					>> "$patchlocal"
	echo "k D"					>> "$patchlocal"

	if [ "6.5" = "$osrel" ]; then
	    dists="foundation applications dev pcp patches"
	else
	    dists="foundation applications dev pcp dwb performer mineset \
		   cosmo snmp freeware softwindows95 patches"
	fi

	# Upgrade/install all other packages from other new distributions
	# in symlink mode, keeping all local packages unchanged.
	for dist in $dists ; do
	    linkpkgs="$LINKPACKAGES.$dist"
	    echo "i A"					>> "$linkpkgs"

	    # Deselect local packages.
	    echo "k c++_eoe.sw.lib"			>> "$linkpkgs"
	    echo "k compiler_eoe.sw.cpp"		>> "$linkpkgs"
	    echo "k compiler_eoe.sw.lboot"		>> "$linkpkgs"
	    echo "k compiler_eoe.sw.lib"		>> "$linkpkgs"
	    echo "k compiler_eoe.sw.unix"		>> "$linkpkgs"
	    echo "k dmedia_eoe.sw.base"			>> "$linkpkgs"
	    echo "k eoe.sw.base"			>> "$linkpkgs"
	    echo "k eoe.sw.efs"				>> "$linkpkgs"
	    echo "k eoe.sw.gfx"				>> "$linkpkgs"
	    echo "k x_eoe.sw.Server"			>> "$linkpkgs"
	    echo "k x_eoe.sw.Xfonts"			>> "$linkpkgs"
	    echo "k x_eoe.sw.eoe"			>> "$linkpkgs"
	    echo "k dmedia_eoe.sw.audio"		>> "$linkpkgs"
	    echo "k dmedia_eoe.sw.lib"			>> "$linkpkgs"
	    echo "k eoe.sw.named"			>> "$linkpkgs"
	    echo "k nfs.sw.nfs"				>> "$linkpkgs"
	    echo "k eoe.sw.terminfo"			>> "$linkpkgs"
	    echo "k eoe.sw.termbase"			>> "$linkpkgs"
	    echo "k x_eoe.sw.Server"			>> "$linkpkgs"
	    echo "k x_eoe.sw.Xapps"			>> "$linkpkgs"
	    echo "k x_eoe.sw.Xfonts"			>> "$linkpkgs"
	    echo "k x_eoe.sw.Xgifts"			>> "$linkpkgs"
	    echo "k x_eoe.sw.Xoptfonts"			>> "$linkpkgs"
	    echo "k x_eoe.sw.Xopttype1"			>> "$linkpkgs"
	    echo "k x_eoe.sw.Xunicodefonts"		>> "$linkpkgs"
	    echo "k x_eoe.sw.eoe"			>> "$linkpkgs"
	    echo "k x_eoe.sw.pex"			>> "$linkpkgs"
	    echo "k x_eoe.sw.plugin"			>> "$linkpkgs"
	    echo "k x_eoe.sw.xfwp"			>> "$linkpkgs"
	    echo "k desktop_base.sw.FileTypingRules"	>> "$linkpkgs"
	    echo "k desktop_base.sw.dso"		>> "$linkpkgs"
	    echo "k desktop_base.sw.ftrdso6_2"		>> "$linkpkgs"
	    echo "k desktop_base.sw.utilities"		>> "$linkpkgs"
	    echo "k ViewKit_eoe.sw.base"		>> "$linkpkgs"
	    echo "k 4Dwm.sw.4Dwm"			>> "$linkpkgs"
	    echo "k motif_eoe.sw.eoe"			>> "$linkpkgs"
	    echo "k websupport_eoe.sw.lib"		>> "$linkpkgs"
	    echo "k ifl_eoe.sw.c++"			>> "$linkpkgs"
	    echo "k il_eoe.sw.c++"			>> "$linkpkgs"
	    echo "k insight_base.sw.eoe"		>> "$linkpkgs"
	    echo "k insight_base.sw.base"		>> "$linkpkgs"
	    echo "k isdn_eoe.sw.isdn"			>> "$linkpkgs"
	    echo "k eoe.sw.ppp"				>> "$linkpkgs"
	    echo "k eoe.sw.uucp"			>> "$linkpkgs"
	    echo "k A ip32prom.sw.prom"			>> "$linkpkgs"
	    echo "k x_eoe.sw.xdps"			>> "$linkpkgs"
	    echo "k dps_eoe.sw.dpsfonts"		>> "$linkpkgs"
	    echo "k desktop_eoe.sw.Confidence"		>> "$linkpkgs"
	    echo "k desktop_eoe.sw.Desks"		>> "$linkpkgs"
	    echo "k desktop_eoe.sw.control_panels"	>> "$linkpkgs"
	    echo "k desktop_eoe.sw.envm"		>> "$linkpkgs"
	    echo "k desktop_eoe.sw.share"		>> "$linkpkgs"
	    echo "k desktop_eoe.sw.toolchest"		>> "$linkpkgs"
	    echo "k desktop_base.sw.dso6_2"		>> "$linkpkgs"
	    echo "k media_warehouse.sw.viewers"		>> "$linkpkgs"
	    echo "k webviewer.sw.eoe"			>> "$linkpkgs"
	    echo "k patchSG0003913.eoe_sw.base"		>> "$linkpkgs"

	    # Deselect packages that are the same version or a downgrade
	    # from what is installed, and overlays of packages we
	    # have not installed.
	    echo "k same"				>> "$linkpkgs"
	    echo "k D"					>> "$linkpkgs"
	    echo "k incompleteoverlays"			>> "$linkpkgs"

	    # Deselect packages which cause conflict when selected
	    # in the wrong pass.
	    echo "k conflicting desktop_base.sw.dso6_2"	>> "$linkpkgs"
	    echo "k conflicting inventor_dev"		>> "$linkpkgs"
	    echo "k conflicting inventor_eoe"		>> "$linkpkgs"
	    echo "k conflicting inventor_games"		>> "$linkpkgs"
	    echo "k conflicting x_eoe.sw.xdps"		>> "$linkpkgs"

	    # Deselect packages causing conflicts, or that we just
	    # don't want.
	    echo "k *.books.*"				>> "$linkpkgs"
	    echo "k InPerson.sw.japanese"		>> "$linkpkgs"
	    echo "k MineSet_addendum_books.addendum.MineSet_UG_A" \
							>> "$linkpkgs"
	    echo "k MineSet_addendum_tutorial.addendum.MineSet_T_A" \
							>> "$linkpkgs"
	    echo "k MineSet_chinese_s_examples.sw.schinese" \
							>> "$linkpkgs"
	    echo "k MineSet_chinese_t_examples.sw.tchinese" \
							>> "$linkpkgs"
	    echo "k MineSet_japanese_examples.sw.japanese" \
							>> "$linkpkgs"
	    echo "k MineSet_korean_examples.sw.korean"	>> "$linkpkgs"
	    echo "k MineSet_tutorial.tutorial.MineSet_T">> "$linkpkgs"
	    echo "k Register"				>> "$linkpkgs"
	    echo "k SoftWindows95.sw.help"		>> "$linkpkgs"
	    echo "k acrobat"				>> "$linkpkgs"
	    echo "k annotator.sw.japanese"		>> "$linkpkgs"
	    echo "k cosmoworlds.sw.chinese_china"	>> "$linkpkgs"
	    echo "k cosmoworlds.sw.chinese_taiwan"	>> "$linkpkgs"
	    echo "k cosmoworlds.sw.japanese"		>> "$linkpkgs"
	    echo "k eoe.src.gnutools"			>> "$linkpkgs"
	    echo "k fw*"				>> "$linkpkgs"
	    echo "k fw_exmh"				>> "$linkpkgs"
	    echo "k fw_turn.man.pict"			>> "$linkpkgs"
