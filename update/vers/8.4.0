	    echo "k gnu"				>> "$linkpkgs"
	    echo "k infosearch.index"			>> "$linkpkgs"
	    echo "k inventor_eoe.sw.help"		>> "$linkpkgs"
	    echo "k java_plugin.sw.swing101"		>> "$linkpkgs"
	    echo "k java_plugin.sw.swing102"		>> "$linkpkgs"
	    echo "k netscape.sw.configs"		>> "$linkpkgs"
	done

	echo "/etc/init.d/network.O"			>> "$CONFIGVERS"
	echo "/etc/config/miser.options.N"		>> "$CONFIGVERS"
	echo "/etc/inetd.conf.O"			>> "$CONFIGVERS"
	echo "/etc/init.d/network.O"			>> "$CONFIGVERS"
	echo "/etc/sendmail.cf.N"			>> "$CONFIGVERS"
	echo "/etc/services.N"				>> "$CONFIGVERS"
	echo "/etc/syslog.conf.N"			>> "$CONFIGVERS"
	echo "/var/netscape/communicator/mime.types.O"	>> "$CONFIGVERS"
	echo "/var/netscape/fasttrack/admin-serv/config/ns-admin.conf.N" \
							>> "$CONFIGVERS"
	echo "/var/netscape/fasttrack/install/ssusers.conf.N" \
							>> "$CONFIGVERS"
	echo "/var/netscape/fasttrack/userdb/certmap.conf.N" \
							>> "$CONFIGVERS"
	echo "/var/netscape/fasttrack/userdb/dbswitch.conf.N" \
							>> "$CONFIGVERS"
	echo "/var/netscape/fasttrack/userdb/ldap/config/lcache.conf.N" \
							>> "$CONFIGVERS"
	echo "/var/spool/cron/crontabs/root.N"		>> "$CONFIGVERS"

	# We want to create new documentation trees.
	echo "UPDATEDOC=true"				>> "$CONFVARS"

	# Clean up dangling links to doc trees.
	echo "/usr/share/help/SGImeeting.helpmap"	>> "$DEADFILES"
	echo "/usr/share/Insight/library/SGI_bookshelves/SGI_Developer/books/InProPub_UG/styles/README" \
							>> "$DEADFILES"
	echo "/usr/share/Insight/library/SGI_bookshelves/SGI_Developer/books/InProPub_UG/figures/02.prepare-1.gif" \
							>> "$DEADFILES"
	echo "/usr/share/Insight/library/SGI_bookshelves/SGI_Developer/books/InProPub_UG/figures/04.debug-2.gif" \
							>> "$DEADFILES"
	echo "/usr/share/Insight/library/SGI_bookshelves/SGI_Developer/books/InProPub_UG/figures/04.debug-1.gif" \
							>> "$DEADFILES"
