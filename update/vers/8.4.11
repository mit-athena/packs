#!/bin/sh

case "$HOSTTYPE" in
sun4)
  if [ -h /usr/vice/etc/CellServDB ]; then
    rm -f /usr/vice/etc/CellServDB
    cp -p /usr/vice/etc/CellServDB.public /usr/vice/etc/CellServDB
    sync
  fi
  chown root:root /usr/vice/etc/CellServDB /usr/vice/etc/CellServDB.public
  chown root:root /usr/vice/etc/SuidCells /usr/vice/etc/SuidCells.public
  chown root:root /usr/vice/etc/ThisCell
  ;;

sgi)
  ;;

esac
