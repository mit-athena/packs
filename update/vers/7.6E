#!/bin/sh -
# Version-specific update script for 7.6E

echo "NEWTTYS=true"			>> ${CONFVARS}

case ${MACH} in
VS2*)	;;
DS*)
	;;
RSAIX)
	;;
SUN*)
	echo "NEWUNIX=true"                     >> ${CONFVARS}
	sed s/RSAIX/SUN4/ < /etc/athena/rc.conf > /tmp/rc.conf
	cp -p /tmp/rc.conf /etc/athena/rc.conf
	rm -f /tmp/rc.conf
esac
