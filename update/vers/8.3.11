#!/bin/sh

case "$HOSTTYPE" in
sun4)
	echo "/etc/inet/inetd.conf"			>> "$CONFCHG"
	;;

sgi)
	# Remove acrobat packages.
	echo "remove acrobat"		>> "$LINKPACKAGES.applications"
	;;
esac

echo "/etc/ifhp.conf"					>> "$CONFCHG"
echo "/etc/lpd.conf"					>> "$CONFCHG"
echo "/etc/lpd.perms"					>> "$CONFCHG"
echo "/.cshrc"						>> "$CONFCHG"
