#!/bin/sh -
# Version-specific update script for 7.7V

case ${MACH} in
DS*)
	;;
RSAIX)
	;;
SUN*)
	echo "NEWUNIX=true"				>> ${CONFVARS}
	echo "D10audio"					>> ${AUXDEVS}
	echo "D20appletalk"				>> ${AUXDEVS}
	echo "Removing S88sendmail and K57sendmail"
	rm -f /etc/rc2.d/S88sendmail
	rm -f /etc/rc0.d/K57sendmail
	rm -f /etc/rc1.d/K57sendmail
	;;
esac
