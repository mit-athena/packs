#!/bin/sh -
# Version-specific update script for 7.7H

echo "NEWTTYS=true"                     >> ${CONFVARS}

case ${MACH} in
DS*)
	;;
RSAIX)
	;;
SUN*)
	if [ -c /dev/audio ]; then
	  chmod 0666 /dev/audio
	fi
	if [ -c /dev/audioctl ]; then
	  chmod 0666 /dev/audioctl
	fi
	rm -f /etc/rc2.d/S92volmgt
	rm -f /etc/rc2.d/K92volmgt
	;;
esac
